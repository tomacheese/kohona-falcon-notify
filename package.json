    "eslint": "9.13.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-n": "17.11.1",
    "eslint-plugin-promise": "7.1.0",
    "prettier": "3.3.3",
    "run-z": "2.1.0",
    "tsx": "4.19.2",
    "twitter-d": "0.6.0",
